generator client {
  provider = "prisma-client-js"
  output   = "../src/generated" 
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Jadwal {
  id      Int      @id  @default(autoincrement())
  bidang  String  
  nama    String
  harga   Int
  jmlHari Int
  bulan   String?
  tanggal String?
}

model DataPendaftar {
  id Int @id @default(autoincrement())
  institusi String
  alamat String
  telepon String
  fax String
  email String
  namaPeserta String
  telPeserta String
  emailPeserta String
  pelatihan String
}
